<p id="notice"><%= notice %></p>

<h1>Listing Members</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Telephone</th>
      <th>Age</th>
      <th>Email</th>
      <th>Address</th>
      <th>Image</th>
      <th>Gender</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.telephone %></td>
        <td><%= member.age %></td>
        <td><%= member.email %></td>
        <td><%= member.address %></td>
        <td><%= member.image %></td>
        <td><%= member.gender %></td>
        <td><%= link_to 'Show',  group_member_path(id: member.id) %></td>
        <td><%= link_to 'Edit', edit_group_member_path(id: member.id) %></td>
        <td><%= link_to 'Destroy', group_member_path(id:member.id), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_group_member_path %>
